<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="largecategory">
	<!-- 전체 리스트 가져오기 -->
	<select id="getLargeCategoryList" resultType="kr.co.forearlybird.domain.LargeCategory">
		SELECT * FROM largecategory
	</select>
	<!-- 컬럼 생성 -->
	<insert id="makeLargeCategory" parameterType="java.lang.String">
		INSERT INTO largecategory large_name VALUES #{largeCategoryName};
	</insert>
</mapper>